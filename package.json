{
  "name": "vue-component-id",
  "version": "0.1.0",
  "description": "Generates an unique ID for every component.",
  "keywords": [
    "vue-plugin",
    "vue",
    "id",
    "id-generator"
  ],
  "cdn": "dist/vue-component-id.js",
  "main": "dist/vue-component-id.cjs.js",
  "types": "src/index.d.ts",
  "unpkg": "dist/vue-component-id.js",
  "module": "dist/vue-component-id.es.js",
  "jsdelivr": "dist/vue-component-id.js",
  "umd:main": "dist/vue-component-id.js",
  "scripts": {
    "test": "ava src/index.spec.js",
    "build": "bili src/index.js --format es,cjs,umd --js buble --banner",
    "pretest": "yarn build"
  },
  "files": [
    "dist/",
    "src/index.d.ts"
  ],
  "author": {
    "url": "https://vitorluizc.github.io/",
    "name": "Vitor Luiz Cavalcanti",
    "email": "vitorluizc@outlook.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VitorLuizC/vue-component-id.git"
  },
  "bugs": {
    "url": "https://github.com/VitorLuizC/vue-component-id/issues"
  },
  "ava": {
    "require": [
      "./src/test/setup-browser-env.js"
    ]
  },
  "homepage": "https://github.com/VitorLuizC/vue-component-id#readme",
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.25",
    "ava": "^0.25.0",
    "bili": "^3.1.2",
    "browser-env": "^3.2.5",
    "vue": "^2.5.17",
    "vue-template-compiler": "^2.5.17"
  }
}
